{
  "env": {
  "es6": true,
  "browser": true,
  "es2021": true
  },
  "parser": "@typescript-eslint/parser", // Specifies the ESLint parser
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
 "rules": {
    "prettier/prettier": "error",
    "bracketSpacing": 0,
    "react/react-in-jsx-scope": "off"
  },
  "plugins": ["prettier"],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
